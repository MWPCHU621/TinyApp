<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>
  <!-- Add your EJS code here -->
  <header>
    <% include ./partials/_header%>
  </header>


  <h1> Homepage </h1>

  <p> <%= urls.length %></p>

  <ul>
    <% for(let url in urls) { %>
      <li>
        <%=url%> -> <a href="<%= urls[url]%>"> <%= urls[url]%></a>
         <a href="/urls/<%=url%>" style="padding-left:10px">Edit</a>
        <form method="POST" action="/urls/<%=url%>/delete"><button type="submit">Delete</button></form>

      </li>
    <% }; %>
  </ul>



  <a href = "#">Shortened URL</a>

</body>
</html>\


